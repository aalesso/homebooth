<div class="page-header">
  <h1>
    Item tags
    <a href="/item_tags/new" class="btn btn-lg btn-success">New Item tag</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Tags</th>
        <th>Item</th>
        <th>Actions</th>
      </tr>

      <% @item_tags.each do |item_tag| %>
      <tr>
        <td><%= item_tag.tags_id %></td>
        <td><%= item_tag.item_id %></td>
        <td>
          <a href="/item_tags/<%= item_tag.id %>" class="btn btn-primary">Show</a>
          <a href="/item_tags/<%= item_tag.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_item_tag/<%= item_tag.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
